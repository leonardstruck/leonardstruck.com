{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "types": {
      "outputs": ["src/payload-types.ts"]
    },
    "build": {
      "dependsOn": ["^build", "types"],
      "env": ["PAYLOAD_*", "NEXT_IMAGE_DOMAINS"],
      "outputs": [".next/**", "!.next/cache/**", "build/**", "dist/**"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
